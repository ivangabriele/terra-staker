    "@ivangabriele/commitlint-config": "1.0.6",
    "@ivangabriele/eslint-config-base": "4.1.1",
    "@ivangabriele/prettier-config": "3.0.2",
    "dotenv": "16.0.1",
    "eslint": "8.0.0",
    "eslint-config-airbnb": "19.0.4",
